window-padding-y = 0
font-family = "IosevkaTerm Nerd Font"
font-size = 13
theme = light:GruvboxLight,dark:GruvboxDark
bold-is-bright = true
mouse-hide-while-typing = true
command = tmux
mouse-scroll-multiplier = 0.01
keybind = ctrl+minus=decrease_font_size:0.5
keybind = ctrl+equal=increase_font_size:0.5

# Unbind keybinds
keybind = unconsumed:alt+1=goto_tab:1
keybind = unconsumed:alt+2=goto_tab:2
keybind = unconsumed:alt+3=goto_tab:3
keybind = unconsumed:alt+4=goto_tab:4
keybind = unconsumed:alt+5=goto_tab:5
keybind = unconsumed:alt+6=goto_tab:6
keybind = unconsumed:alt+7=goto_tab:7
keybind = unconsumed:alt+8=goto_tab:8
keybind = unconsumed:alt+9=last_tab
